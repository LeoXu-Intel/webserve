{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Multiselect from 'vue-multiselect';\nimport configData from '@/assets/config.json';\nexport default {\n  components: {\n    Multiselect\n  },\n  data() {\n    const forms = this.initializeForms(configData);\n    return {\n      activeName: 'first',\n      configData,\n      forms,\n      selection1: '',\n      selection2: '',\n      selection3: '',\n      selection4: [],\n      domains: [{\n        name: 'Basic'\n      }, {\n        name: 'QAT'\n      }, {\n        name: 'DLB'\n      }, {\n        name: 'DPDK'\n      }]\n    };\n  },\n  created() {\n    this.loadSelectionsFromStore();\n  },\n  computed: {\n    combinedTabsConfig() {\n      return this.getCombinedTabsConfig(this.selection4);\n    }\n  },\n  methods: {\n    initializeForms(configData) {\n      const forms = {};\n      configData.forEach(tab => {\n        forms[tab.name] = {};\n        tab.formItems.forEach(item => {\n          forms[tab.name][item.model] = '';\n        });\n      });\n      return forms;\n    },\n    getCombinedTabsConfig(selection) {\n      let combinedConfig = [configData.find(tab => tab.label === 'Basic')];\n      selection.forEach(domainLabel => {\n        let domainConfig = configData.find(tab => tab.label === domainLabel);\n        if (domainConfig) {\n          combinedConfig.push(domainConfig);\n        }\n      });\n      return combinedConfig;\n    },\n    loadSelectionsFromStore() {\n      // 假设你有一个 action 来加载数据\n      this.$store.dispatch('loadSelections').then(() => {\n        const selections = this.$store.state.selections;\n        this.selection1 = selections.selection1 || '';\n        this.selection2 = selections.selection2 || '';\n        this.selection3 = selections.selection3 || '';\n        this.selection4 = selections.selection4 || [];\n      });\n    },\n    navigateToOtherPage() {\n      this.$router.push('/other-page');\n    }\n  }\n};","map":{"version":3,"names":["Multiselect","configDat