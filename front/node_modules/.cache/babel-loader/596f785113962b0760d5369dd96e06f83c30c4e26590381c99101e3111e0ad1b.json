el=\"selection4\"\r\n          :options=\"domains\"\r\n          :multiple=\"true\"\r\n          :taggable=\"true\"\r\n          :close-on-select=\"false\"\r\n          :clear-on-select=\"false\"\r\n          :preserve-search=\"true\"\r\n          placeholder=\"Pick some\"\r\n          label=\"name\"\r\n          track-by=\"name\"\r\n          :preselect-first=\"false\"\r\n        ></multiselect>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"row button-row\">\r\n      <button @click=\"search\">Search</button>\r\n    </div>\r\n\r\n  </div>\r\n\r\n  \r\n  <div style=\"display: flex; justify-content: center; margin-top: 10px;\">\r\n    <el-tabs v-model=\"activeName\" type=\"card\" @tab-click=\"handleClick\" style=\"width: 50%;\">\r\n      <!-- 用户管理标签页 -->\r\n      <el-tab-pane label=\"用户管理\" name=\"first\">\r\n        <el-form>\r\n          <el-form-item label=\"用户名\">\r\n            <el-input v-model=\"userForm.username\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"邮箱\">\r\n            <el-input v-model=\"userForm.email\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"密码\">\r\n            <el-input type=\"password\" v-model=\"userForm.password\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-tab-pane>\r\n      <!-- 配置管理标签页 -->\r\n      <el-tab-pane label=\"配置管理\" name=\"second\">\r\n        <el-form>\r\n          <el-form-item label=\"配置项1\">\r\n            <el-input v-model=\"configForm.configItem1\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"配置项2\">\r\n            <el-input v-model=\"configForm.configItem2\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-tab-pane>\r\n      <!-- 角色管理标签页 -->\r\n      <el-tab-pane label=\"角色管理\" name=\"third\">\r\n        <el-form>\r\n          <el-form-item label=\"角色名称\">\r\n            <el-input v-model=\"roleForm.roleName\"></el-input>\r\n          </el-form-item>\r\n     