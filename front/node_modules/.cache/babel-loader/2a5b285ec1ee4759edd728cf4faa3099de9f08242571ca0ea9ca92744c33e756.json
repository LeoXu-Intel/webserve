          <el-select v-model=\"value1\" multiple placeholder=\"请选择\">\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\">\r\n            </el-option>\r\n          </el-select>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"checkbox-groups-container\">\r\n    <!-- 第一组多选框 -->\r\n    <div class=\"checkbox-group\">\r\n      <el-scrollbar style=\"height: 200px;\">\r\n        <el-checkbox-group v-model=\"checkedGroup1\">\r\n          <el-checkbox\r\n            v-for=\"item in options1\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            border>\r\n          </el-checkbox>\r\n        </el-checkbox-group>\r\n      </el-scrollbar>\r\n    </div>\r\n    <!-- 第二组多选框 -->\r\n    <div class=\"checkbox-group\">\r\n      <el-scrollbar style=\"height: 200px;\">\r\n        <el-checkbox-group v-model=\"checkedGroup2\">\r\n          <el-checkbox\r\n            v-for=\"item in options2\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            border>\r\n          </el-checkbox>\r\n        </el-checkbox-group>\r\n      </el-scrollbar>\r\n    </div>\r\n  </div>\r\n    \r\n\r\n  <div class=\"button-container\">\r\n      <button @click=\"Build\">Build</button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Multiselect from 'vue-multiselect';\r\n\r\nexport default {\r\n  components: { Multiselect },\r\n  created() {\r\n    this.loadSelections();\r\n  },\r\n  data() {\r\n    return {\r\n      selection1: '',\r\n      selection2: '',\r\n      selection3: '',\r\n      selection4: [],\r\n      options: [{\r\n          value: 'QAT',\r\n          label: 'QAT'\r\n        }, {\r\n          value: 'DLB',\r\n          label: 'DLB'\r\n        }, {\r\n          value: 'DSA',\r\n          label: 'DSA'\r\n        }, {\r\n          value: 'Networking',\r\n          label: 'Networking'\