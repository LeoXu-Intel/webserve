{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Multiselect from 'vue-multiselect';\nimport configData from '@/assets/config.json';\nexport default {\n  components: {\n    Multiselect\n  },\n  data() {\n    const forms = {};\n    configData.forEach(tab => {\n      forms[tab.name] = {};\n      tab.formItems.forEach(item => {\n        forms[tab.name][item.model] = '';\n      });\n    });\n    return {\n      activeName: 'first',\n      configData,\n      forms,\n      selection4: [],\n      domains: [{\n        name: 'Networking'\n      }, {\n        name: 'QAT'\n      }, {\n        name: 'DLB'\n      }, {\n        name: 'DPDK'\n      }]\n    };\n  },\n  computed: {\n    combinedTabsConfig() {\n      let combinedConfig = [configData.find(tab => tab.label === 'Basic')];\n      this.selection4.forEach(domainName => {\n        let domainConfig = configData.find(tab => tab.label === domainName);\n        if (domainConfig) {\n          combinedConfig.push(domainConfig);\n        }\n      });\n      return combinedConfig;\n    }\n  },\n  watch: {\n    selection4: {\n      handler() {\n        // 当selection4变化时，更新forms数据\n        this.updateFormsData();\n      },\n      deep: true\n    }\n  },\n  methods: {\n    updateFormsData() {\n      // 根据combinedTabsConfig更新forms对象\n      const newForms = {};\n      this.combinedTabsConfig.forEach(tab => {\n        newForms[tab.name] = this.forms[tab.name] || {};\n        tab.formItems.forEach(item => {\n          if (newForms[tab.name][item.model] === undefined) {\n            newForms[tab.name][item.model] = ''; // 设置默认值\n          } else {\n            // 保留已有值\n            newForms[tab.name][item.model] = this.forms[tab.name][item.model];\n          }\n        });\n      });\n      this.forms = newForms;\n    },\n    handleClick(tab, event) {\n      // 处理标签点击事件\n    },\n    navigateToOtherPage() {\n      // 实现跳转逻辑\n      this.$router.push('/other-page');\n    }\n  }\n};","map":{"version":3,"names":["Multiselect","configData","components","data","forms","forEach","tab","name","formItems","item","model","activeName","selection4","domains","computed","combinedTabsConfig","combinedConfig","find","label","domainName","domainConfig","push","watch","handler","updateFormsData","deep","methods","newForms","undefined","handleClick","event","navigateToOtherPage","$router"],"sources":["C:\\Users\\axu\\OneDrive - Intel Corporation\\Desktop\\Web serve\\webserve-0320\\webserve\\front\\src\\components\\LabTools\\Sensor\\JenkinsAPI-P.vue"],"sourcesContent":["<template>\r\n<div style=\"display: flex; width: 100%; justify-content: center;\">\r\n  <el-steps :active=\"1\" finish-status=\"success\" simple style=\"width: 60%; margin-top: 20px;\">\r\n    <el-step title=\"Test Case Overview\"></el-step>\r\n    <el-step title=\"Environment Configuration\"></el-step>\r\n    <el-step title=\"Aotumation Configuration\"></el-step>\r\n  </el-steps>\r\n</div>\r\n\r\n  <div class=\"search-form\">\r\n    <div class=\"row\">\r\n\r\n      <div class=\"input-group\">\r\n        <label for=\"select1\">Platform:</label>\r\n        <select id=\"select1\" v-model=\"selection1\">\r\n          <option value=\"option1\">SP1</option>\r\n          <option value=\"option2\">SP2</option>\r\n          <!-- 更多选项 -->\r\n        </select>\r\n      </div>\r\n\r\n      <div class=\"input-group\">\r\n        <label for=\"select2\">Test_Cycle_Name:</label>\r\n        <select id=\"select2\" v-model=\"selection2\">\r\n          <option value=\"option1\">Test_Cycle_1</option>\r\n          <option value=\"option2\">Test_Cycle_2</option>\r\n          <!-- 更多选项 -->\r\n        </select>\r\n      </div>\r\n\r\n    </div>\r\n\r\n    <div class=\"row\">\r\n\r\n      <div class=\"input-group\">\r\n        <label for=\"select3\">Cycle_Config:</label>\r\n        <select id=\"select3\" v-model=\"selection3\">\r\n          <option value=\"option1\">Cycle_Config_1</option>\r\n          <option value=\"option2\">Cycle_Config_2</option>\r\n          <!-- 更多选项 -->\r\n        </select>\r\n      </div>\r\n\r\n      <div class=\"input-group\">\r\n        <label for=\"select4\">Test_Domain:</label>\r\n            <multiselect\r\n              id=\"select4\"\r\n              v-model=\"selection4\"\r\n              :options=\"domains\"\r\n              :multiple=\"true\"\r\n              :taggable=\"true\"\r\n              :close-on-select=\"false\"\r\n              :clear-on-select=\"false\"\r\n              :preserve-search=\"true\"\r\n              placeholder=\"Pick some\"\r\n              label=\"name\"\r\n              track-by=\"name\"\r\n              :preselect-first=\"false\"\r\n              @input=\"handleSelectionChange\"\r\n            ></multiselect>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n    <!-- <div class=\"row button-row\">\r\n      <button @click=\"search\">Search</button>\r\n    </div> -->\r\n\r\n\r\n   <!-- 查询结果列表 -->\r\n  <div class=\"tabs-container\">\r\n    <el-tabs v-model=\"activeName\" type=\"card\" @tab-click=\"handleClick\">\r\n      <el-tab-pane\r\n        v-for=\"tab in combinedTabsConfig\"\r\n        :key=\"tab.name\"\r\n        :label=\"tab.label\"\r\n        :name=\"tab.name\"\r\n      >\r\n        <el-form label-position=\"top\">\r\n          <el-form-item\r\n            v-for=\"item in tab.formItems\"\r\n            :key=\"item.model\"\r\n            :label=\"item.label\"\r\n          >\r\n            <el-input\r\n              :type=\"item.type\"\r\n              v-model=\"forms[tab.name][item.model]\"\r\n            />\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-tab-pane>\r\n    </el-tabs>\r\n  </div> \r\n\r\n  <!-- 省略其他模板内容 -->\r\n</template>\r\n\r\n<script>\r\nimport Multiselect from 'vue-multiselect';\r\nimport configData from '@/assets/config.json';\r\n\r\nexport default {\r\n  components: {\r\n    Multiselect\r\n  },\r\n  data() {\r\n    const forms = {};\r\n    configData.forEach(tab => {\r\n      forms[tab.name] = {};\r\n      tab.formItems.forEach(item => {\r\n        forms[tab.name][item.model] = '';\r\n      });\r\n    });\r\n\r\n    return {\r\n      activeName: 'first',\r\n      configData,\r\n      forms,\r\n      selection4: [],\r\n      domains: [\r\n        { name: 'Networking' },\r\n        { name: 'QAT' },\r\n        { name: 'DLB' },\r\n        { name: 'DPDK' }\r\n      ],\r\n    };\r\n  },\r\n  computed: {\r\n    combinedTabsConfig() {\r\n      let combinedConfig = [configData.find(tab => tab.label === 'Basic')];\r\n      this.selection4.forEach(domainName => {\r\n        let domainConfig = configData.find(tab => tab.label === domainName);\r\n        if (domainConfig) {\r\n          combinedConfig.push(domainConfig);\r\n        }\r\n      });\r\n      return combinedConfig;\r\n    }\r\n  },\r\n  watch: {\r\n    selection4: {\r\n      handler() {\r\n        // 当selection4变化时，更新forms数据\r\n        this.updateFormsData();\r\n      },\r\n      deep: true\r\n    }\r\n  },\r\n  methods: {\r\n    updateFormsData() {\r\n      // 根据combinedTabsConfig更新forms对象\r\n      const newForms = {};\r\n      this.combinedTabsConfig.forEach(tab => {\r\n        newForms[tab.name] = this.forms[tab.name] || {};\r\n        tab.formItems.forEach(item => {\r\n          if (newForms[tab.name][item.model] === undefined) {\r\n            newForms[tab.name][item.model] = ''; // 设置默认值\r\n          } else {\r\n            // 保留已有值\r\n            newForms[tab.name][item.model] = this.forms[tab.name][item.model];\r\n          }\r\n        });\r\n      });\r\n      this.forms = newForms;\r\n    },\r\n    handleClick(tab, event) {\r\n      // 处理标签点击事件\r\n    },\r\n    navigateToOtherPage() {\r\n      // 实现跳转逻辑\r\n      this.$router.push('/other-page');\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.tabs-container {\r\n  width: 600px; /* 固定宽度 */\r\n  height: 300px; /* 高度根据内容自动调整 */\r\n  margin: 20px auto; /* 上下边距和自动左右边距 */\r\n  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1); /* 添加阴影效果 */\r\n  border-radius: 4px; /* 圆角