LevelList\"\n                    :key = \"index\"\n                    :label = \"item\"\n                    :value = \"item\"\n                />\n            </el-select> -->\n        </el-col>\n\n\n    </el-row>\n\n\n    <el-divider border-style=\"dashed\" />\n    \n    <!-- 针对code -->\n    <el-row :gutter=\"10\" justify=\"space-evenly\" align=\"middle\">\n        <el-col :span=\"6\" align=\"center\">\n            <el-text tag=\"b\" size=\"large\">Code</el-text>\n            <el-input v-model=\"code\" :readonly=\"false\" />\n        </el-col>\n\n    <!-- 针对Finding -->\n    <el-col :span=\"12\" align=\"center\" >\n        <el-text tag=\"b\" size=\"large\">Finding</el-text>\n        <el-input \n            v-model=\"finding\" \n            :readonly=\"false\"\n            size=\"large\"\n            :autosize=\"{ minRows: 2, maxRows: 4 }\"\n            type=\"textarea\"\n        />\n        </el-col>\n    </el-row>\n\n\n    <el-divider border-style=\"dashed\" />\n\n    <!-- 针对Criteria -->\n    <el-row justify=\"space-evenly\" align=\"middle\">\n        <el-col :span=\"20\" align=\"center\" >\n           <el-text tag=\"b\" size=\"large\">Criteria</el-text>\n            <el-input \n                v-model=\"criteria\" \n                :readonly=\"false\"\n                size=\"large\"\n                :autosize=\"{ minRows: 5, maxRows: 8 }\"\n                type=\"textarea\"\n            /> \n        </el-col>\n    </el-row>\n\n    <el-divider border-style=\"dashed\" />\n\n\n    <!-- 针对Assessment -->\n    <el-row justify=\"space-evenly\" align=\"middle\">\n        <el-col :span=\"20\" align=\"center\" >\n           <el-text tag=\"b\" size=\"large\">Assessment</el-text>\n            <el-input \n                v-model=\"assessment\" \n                :readonly=\"false\"\n                size=\"large\"\n                :autosize=\"{ minRows: 5, maxRows: 8 }\"\n                type=\"textarea\"\n            /> \n        </el-col>\n\n        <el-divider border-style=