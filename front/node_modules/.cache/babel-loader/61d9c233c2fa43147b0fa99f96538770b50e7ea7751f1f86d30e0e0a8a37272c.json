eft: 10%; /* 根据需要调整标签右边距 */\r\n  white-space: nowrap;\r\n  font-weight: bold; /* 加粗文本 */\r\n}\r\n\r\n.input-group select,\r\n.multiselect {\r\n  flex-grow: 1;\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  min-width: 150px; /* 根据需要调整选择框最小宽度 */\r\n}\r\n\r\n.button-row {\r\n  display: flex;\r\n  justify-content: right;\r\n  padding-top: 2%;\r\n  padding-right: 5%;\r\n}\r\n\r\n.row button {\r\n  padding: 15px 30px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  font-size: 15px;\r\n  font-weight: bold;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.row button:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n/* 调整多选下拉框的长度 */\r\n.multiselect {\r\n  /* display: flex; */\r\n  align-items: center;\r\n  margin-right: 0%;\r\n  margin-left: 7%;\r\n  width: 400px; /* 或者你可以设置一个具体的长度，例如：width: 300px; */\r\n}\r\n\r\n/* 当选项被选中时，改变背景颜色为蓝色 */\r\n.multiselect .multiselect__tag {\r\n  background-color: #007bff; /* 蓝色背景 */\r\n  color: white; /* 白色文字 */\r\n}\r\n/* 当鼠标悬停在选项上时，也改变背景颜色为蓝色 */\r\n.multiselect .multiselect__option--highlight {\r\n  background-color: #007bff; /* 蓝色背景 */\r\n  color: white; /* 白色文字 */\r\n}\r\n.multiselect .multiselect__tag-icon {\r\n  color: white;\r\n}\r\n\r\n/* 当选项被选中时，改变删除图标的颜色为白色 */\r\n.multiselect .multiselect__tag-icon {\r\n  color: white;\r\n}\r\n\r\n/* 调整多选下拉框的选项列表样式 */\r\n.multiselect .multiselect__content-wrapper {\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\n\r\n/* 调整多选下拉框的选项样式 */\r\n.multiselect .multiselect__option--highlight {\r\n  background-color: #007bff;\r\n  color: white;\r\n}\r\n\r\n/* 调整多选下拉框的搜索输入框样式 */\r\n.multiselect .multiselect__input,\r\n.multiselect .multiselect__single {\r\n  height: 38px; /* 根据需要调整高度 */\r\n  padding: 5px;\r\n  width: 150px;\r\n  color: #000;\r\n}\r\n\r\n\r\n/* 调整多选下拉框的箭头样式 */\r\n.multiselect .multiselect__select {\r\n  color: #495057;\r\n}\r\n\r\n</style>"],"mappings":"AAuDA,OAAOA,WAA